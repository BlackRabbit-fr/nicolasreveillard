{
  "name": "nicolas-reveillard",
  "description": "Site de l'artiste peintre Nicolas Réveillard originaire de la Drôme. Ce site regroupe quelques-unes de ces oeuvres en format huiles sur toile et papier.",
  "version": "1.0.0",
  "author": "Théo Réveillard <reveillard.theo@gmail.com>",
  "dependencies": {
    "bulma": "^0.7.5",
    "gatsby-image": "^2.0.38",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-sharp": "^2.0.34",
    "gatsby-source-contentful": "^2.0.48",
    "gatsby-transformer-remark": "^2.3.8",
    "gatsby-transformer-sharp": "^2.1.18",
    "lodash": "^4.17.11",
    "node-sass": "^4.12.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0",
    "react-images": "^1.0.0",
    "react-spring": "^8.0.27",
    "react-visibility-sensor": "^5.1.1",
    "sharp": "^0.22.0"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.9.0",
    "babel-eslint": "^10.0.1",
    "dotenv": "^8.0.0",
    "eslint": "^5.16.0",
    "eslint-plugin-react": "^7.12.4",
    "gatsby": "^2.3.22",
    "gh-pages": "^2.0.1",
    "prettier": "^1.17.0",
    "yargs-parser": "^13.0.0"
  },
  "homepage": "nicolasreveillard.fr",
  "keywords": [
    "nicolas",
    "reveillard",
    "aquarelle",
    "encre",
    "huile sur toile",
    "paysages",
    "drome",
    "nature"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/contentful-userland/gatsby-contentful-starter.git"
  },
  "scripts": {
    "css-build": "node-sass --omit-source-map-url src/sass/styles.scss src/css/mystyles.css",
    "css-watch": "npm run css-build -- --watch",
    "build": "npm run css-build",
    "dev": "npm run develop",
    "develop": "gatsby develop",
    "lint": "eslint --ext .js,.jsx --ignore-pattern public .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write 'src/**/*.js' 'src/**/*.md' 'bin/*.js'",
   // "build": "npm run css-build && gatsby build",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "fix-semi": "eslint --quiet --ignore-pattern node_modules --ignore-pattern public --parser babel-eslint --no-eslintrc --rule '{\"semi\": [2, \"never\"], \"no-extra-semi\": [2]}' --fix *.js bin/*.js",
    "postinstall": "node ./bin/hello.js",
    "setup": "node ./bin/setup.js",
    "start": "npm run develop",
    "heroku-postbuild": "gatsby build",
    "cleanup-repository": "rimraf bin contentful && yarn remove contentful-import chalk inquirer rimraf "
  }
}
